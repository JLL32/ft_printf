{
    "tasks": [
        {
            "type": "shell",
            "label": "clang build active file",
            "command": "/usr/bin/clang",
            "args": [
                "-g",
				"${workspaceFolder}/src/main.c",
				"-L",
				"${workspaceFolder}/src",
				"-lftprintf",
				"-L",
				"${workspaceFolder}/src/libft",
				"-lft",
                "-o",
                "main"
            ],
            "options": {
                "cwd": "/usr/bin"
            }
        },
    ],
    "version": "2.0.0"
}
